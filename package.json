{
  "name": "kalpana-agent",
  "version": "1.1.11",
  "description": "Kalpana (कल्पना) - AI development assistant with multi-runtime containerized execution, web automation, multi-modal analysis, error checking, and intelligent context management",
  "keywords": [
    "ai",
    "assistant",
    "development",
    "docker",
    "automation",
    "web-scraping",
    "multi-modal",
    "error-checking",
    "context-management",
    "nodejs",
    "python",
    "bun"
  ],
  "author": "Kalpana AI Team",
  "license": "MIT",
  "homepage": "https://github.com/Emmanuek5/Kalpana-agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/Emmanuek5/Kalpana-agent.git"
  },
  "bugs": {
    "url": "https://github.com/Emmanuek5/Kalpana-agent/issues"
  },
  "bin": {
    "kalpana": "./dist/src/bin/kalpana.js",
    "kalpana-interactive": "./dist/src/bin/kalpana-interactive.js",
    "kalpana-config": "./dist/src/bin/kalpana-config.js"
  },
  "main": "./dist/index.js",
  "module": "index.ts",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "scripts/",
    "README.md",
    "README-*.md",
    "Dockerfile.bun",
    "system.txt",
    "tsconfig.build.json"
  ],
  "scripts": {
    "start": "bun run src/cli.ts",
    "dev": "bun --hot run src/cli.ts",
    "interactive": "bun run src/cli-interactive.ts",
    "build": "tsc -p tsconfig.build.json && npm run build:fix-imports",
    "build:fix-imports": "node scripts/fix-imports.js",
    "build-image": "docker build -f Dockerfile.bun -t ai-container:multi-runtime .",
    "test-image": "docker run --rm ai-container:multi-runtime node --version && docker run --rm ai-container:multi-runtime bun --version && docker run --rm ai-container:multi-runtime python --version",
    "prepublishOnly": "npm run build",
    "postinstall": "echo 'Kalpana installed! Run: kalpana --help'"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dockerode": "^3.3.44",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@google/genai": "latest",
    "@hyperbrowser/sdk": "^0.61.0",
    "@modelcontextprotocol/sdk": "^1.3.1",
    "@notionhq/client": "^2.2.15",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@types/marked": "^6.0.0",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "ai": "^5.0.48",
    "chalk": "^5.6.2",
    "dockerode": "^4.0.8",
    "dotenv": "^17.2.2",
    "googleapis": "^144.0.0",
    "ink": "^6.3.1",
    "ink-box": "^2.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^16.3.0",
    "marked-terminal": "^7.3.0",
    "openai": "^5.22.0",
    "puppeteer": "^24.22.0",
    "puppeteer-core": "^24.22.0",
    "react": "^19.1.1",
    "zod": "^4.1.11"
  }
}
