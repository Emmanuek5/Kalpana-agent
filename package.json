{
  "name": "ai-container",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/cli.ts",
    "dev": "bun --hot run src/cli.ts",
    "interactive": "bun run src/cli-interactive.ts",
    "build-image": "docker build -f Dockerfile.bun -t ai-container:multi-runtime .",
    "test-image": "docker run --rm ai-container:multi-runtime node --version && docker run --rm ai-container:multi-runtime bun --version && docker run --rm ai-container:multi-runtime python --version"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/dockerode": "^3.3.44"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@google/genai": "latest",
    "@hyperbrowser/sdk": "^0.61.0",
    "@modelcontextprotocol/sdk": "^1.3.1",
    "@notionhq/client": "^2.2.15",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@types/marked": "^6.0.0",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "ai": "^5.0.48",
    "chalk": "^5.6.2",
    "dockerode": "^4.0.8",
    "dotenv": "^17.2.2",
    "googleapis": "^144.0.0",
    "ink": "^6.3.1",
    "ink-box": "^2.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^16.3.0",
    "marked-terminal": "^7.3.0",
    "openai": "^5.22.0",
    "puppeteer": "^24.22.0",
    "puppeteer-core": "^24.22.0",
    "react": "^19.1.1",
    "zod": "^4.1.11"
  }
}
